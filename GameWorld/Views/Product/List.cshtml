@model ProductsListViewModel
<h3 class="text-center text-danger"><b>Gameplay Screenshots</b></h3>
@foreach (var p in Model.Products)
{
    @Html.Partial("ProductSummary", p)
}

<div page-model="@Model.PagingInfo" page-action="List" page-class-enabled="true" page-class="btn" page-class-normal="btn-default" page-class-selected="btn-success"
     page-url-category="@Model.CurrentCategory" class="btn-group pull-right"></div>